{
  "kind": "collectionType",
  "collectionName": "stores",
  "info": {
    "singularName": "store",
    "pluralName": "stores",
    "displayName": "Store",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string"
    },
    "isOpen": {
      "type": "boolean"
    },
    "storeGeoLocation": {
      "type": "component",
      "repeatable": false,
      "component": "geo-location.location"
    },
    "city": {
      "type": "string"
    },
    "zipCode": {
      "type": "string"
    },
    "picture": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "store"
    }
  }
}
